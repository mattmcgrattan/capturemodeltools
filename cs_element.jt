},
{% else %}
"crowds:derivedAnnoMotivatedBy": {
"@id": "{{crowds_derivedAnnoMotivatedBy}}",
"o:label": "{{crowds_derivedAnnoMotivatedBy}}"
},
{% endif %}
"crowds:derivedAnnoMotivatedBy": {
"@id": "oa:tagging",
"o:label": "oa:tagging"
},
{% endif %}
{% if rdfs_range %}
{% if rdfs_range_label %}
"rdfs:range": {
"@id": "{{rdfs_range}}",
"o:label": "{{rdfs_range_label}}"
},
{% else %}
"rdfs:range": {
"@id": "{{rdfs_range}}",
"o:label": "{{rdfs_range}}"
},
{% endif %}
{% else %}
"rdfs:range": {
"@id": "http://www.w3.org/2000/01/rdf-schema#literal",
"o:label": "rdfs:literal"
},
{% endif %}
{% if dcterms_conformsTo %}
{% if dcterms_conformsTo_label %}
"dcterms:conformsTo": {
"@id": "{{dcterms_conformsTo}}",
"o:label": "{{dcterms_conformsTo_label}}"
},
{% else %}
"dcterms:conformsTo": {
"@id": "{{dcterms_conformsTo}}",
"o:label": "{{dcterms_conformsTo}}"
},
{% endif %}
{% endif %}
{% if crowds_uiSelectorType %}
{% if crowds_uiSelectorType_label %}
"crowds:uiSelectorType": {
"@id": "{{crowds_uiSelectorType}}",
"o:label": "{{crowds_uiSelectorType}}"
},
{% else %}
"crowds:uiSelectorType": {
"@id": "{{crowds_uiSelectorType}}",
"o:label": "{{crowds_uiSelectorType}}"
},
{% endif %}
{% endif %}
{% if crowds_uiInputType %}
{% if crowds_uiInputType_label %}
"crowds:uiInputType": {
"@id": "{{crowds_uiInputType}}",
