{
  "@id": "{{url}}/api/items/{{dcterms_identifier}}",
  "o:id": {{dcterms_identifier}},
  "@type": [
    "o:Item",
    "dctype:InteractiveResource"
  ],
"rdfs:label": "{{rdfs_label}}",
"dcterms:title": "{{dcterms_title}}",
{% if dcterms_description %}
"dcterms:description": "{{dcterms_description}}",
{% endif %}
{% if crowds_derivedAnnoBodyFormat %}
"crowds:derivedAnnoBodyFormat": "{{crowds_derivedAnnoBodyFormat}}",
{% else %}
"crowds:derivedAnnoBodyFormat": "text/plain",
{% endif %}
{% if crowds_uiRequired %}
"crowds:uiRequired": "{{crowds_uiRequired}}",
{% else %}
"crowds:uiRequired": "False",
{% endif %}
{% if crowds_derivedAnnoBodyPurpose %}
{% if crowds_derivedAnnoBodyPurpose_label %}
"crowds:derivedAnnoBodyPurpose": {
"@id": "{{crowds_derivedAnnoBodyPurpose}}",
"o:label": "{{crowds_derivedAnnoBodyPurpose_label}}"
},
{% else %}
"crowds:derivedAnnoBodyPurpose": {
"@id": "{{crowds_derivedAnnoBodyPurpose}}",
"o:label": "{{crowds_derivedAnnoBodyPurpose}}"
},
{% endif %}
{% endif %}
{% if crowds_uiSelectorValue %}
"crowds:uiSelectorValue": "{{crowds_uiSelectorValue}}",
{% endif %}
{% if crowds_derivedAnnoBodyType %}
