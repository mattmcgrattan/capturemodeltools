},
{% else %}
"crowds:uiInputType": {
"@id": "{{crowds_uiInputType}}",
"o:label": "{{crowds_uiInputType}}"
},
{% endif %}
{% endif %}
{% if crowds_uiInputOptions %}
"crowds:uiInputOptions": {{crowds_uiInputOption}},
{% endif %}
{% if crowds_uiHidden %}
"crowds:uiHidden": "{{crowds_uiHidden}}",
{% else %}
"crowds:uiHidden": "False",
{% endif %}
{% if crowds_uiFormGroup %}
{% set ui_formgrouplabel =  crowds_uiFormGroup.title() %}
"crowds:uiFormGroup":
{
"@id": "{{crowds_uiFormGroup}}",
"o:label": "{{ui_formgrouplabel}}"
},
{% endif %}
"o:is_public": true,
"o:item_set": [],
