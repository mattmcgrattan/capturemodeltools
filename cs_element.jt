"crowds:derivedAnnoBodyType": {
"@id": "{{crowds_derivedAnnoBodyType}}",
"o:label": "{{crowds_derivedAnnoBodyType_label}}"
},
{% else %}
"crowds:derivedAnnoBodyType": {
"@id": "{{crowds_derivedAnnoBodyType}}",
"o:label": "{{crowds_derivedAnnoBodyType}}"
},
{% endif %}
"crowds:derivedAnnoBodyType": {
"o:label": "https://www.w3.org/ns/oa#TextualBody",
"@id": "oa:TextualBody"
},
{% endif %}
{% if crowds_derivedAnnoMotivatedBy %}
{% if crowds_derivedAnnoMotivatedBy_label %}
"crowds:derivedAnnoMotivatedBy": {
"@id": "{{crowds_derivedAnnoMotivatedBy}}",
